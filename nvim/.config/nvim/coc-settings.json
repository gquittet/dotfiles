{
  "cSpell.language": "en,fr",
  "cSpell.userWords": ["Quittet"],
  "cSpellExt.enableDictionaries": ["french", "medical-terms"],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "json",
    "python",
    "rust",
    "sh",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "coc.preferences.formatOnType": true,
  "coc.source.emoji.filetypes": ["markdown"],
  "codeLens.enable": true,
  "diagnostic-languageserver.filetypes": {
    "markdown": ["write-good", "markdownlint"],
    "sh": "shellcheck"
  },
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "explorer.previewAction.onHover": false,
  "explorer.width": 30,
  "git.enableGutters": true,
  "javascript.referencesCodeLens.enable": false,
  "languageserver": {
    "sourcery": {
      "args": ["lsp"],
      "command": "sourcery",
      "filetypes": ["python"],
      "initializationOptions": {
        "editor_version": "vim",
        "extension_version": "coc.vim",
        "token": "user_PLavqbOyVllZbd3zcY5U9kXuA4ekJiKQ2pSe7rWU_QEP7FrOvrbMEpJnxE4"
      },
      "settings": { "sourcery": { "metricsEnabled": false } }
    }
  },
  "list.source.files.args": [
    "--files",
    "--hidden",
    "--no-ignore-vcs",
    "--follow",
    "--glob",
    "!{.git}"
  ],
  "list.source.files.command": "rg",
  "list.source.grep.args": [
    "--hidden",
    "--ignore-case",
    "--no-ignore-vcs",
    "--follow",
    "--glob",
    "!{.git}"
  ],
  "list.source.grep.command": "rg",
  "tsserver.disableAutomaticTypeAcquisition": false,
  "typescript.referencesCodeLens.enable": false
}
